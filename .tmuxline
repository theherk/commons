# This tmux statusbar config was created by tmuxline.vim
# on Mon, 16 Mar 2015

set -g status-bg "colour251"
set -g message-command-fg "colour238"
set -g status-justify "left"
set -g status-left-length "100"
set -g status "on"
set -g pane-active-border-fg "colour64"
set -g message-bg "colour249"
set -g status-right-length "100"
set -g status-right-attr "none"
set -g message-fg "colour238"
set -g message-command-bg "colour249"
set -g status-attr "none"
set -g status-utf8 "on"
set -g pane-border-fg "colour249"
set -g status-left-attr "none"
setw -g window-status-fg "colour239"
setw -g window-status-attr "none"
setw -g window-status-activity-bg "colour251"
setw -g window-status-activity-attr "none"
setw -g window-status-activity-fg "colour64"
setw -g window-status-separator ""
setw -g window-status-bg "colour251"
set -g status-left "#[fg=colour251,bg=colour64] #S #[fg=colour64,bg=colour251,nobold,nounderscore,noitalics]"
set -g status-right "#[fg=colour249,bg=colour251,nobold,nounderscore,noitalics]#[fg=colour239,bg=colour249] %Y-%m-%d  %H:%M #[fg=colour242,bg=colour249,nobold,nounderscore,noitalics]#[fg=colour251,bg=colour242] #h "
setw -g window-status-format "#[fg=colour239,bg=colour251] #I #[fg=colour239,bg=colour251] #W "
setw -g window-status-current-format "#[fg=colour251,bg=colour249,nobold,nounderscore,noitalics]#[fg=colour238,bg=colour249] #I #[fg=colour238,bg=colour249] #W #[fg=colour249,bg=colour251,nobold,nounderscore,noitalics]"
