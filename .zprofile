export LANG=en_US.UTF-8

export CARGO_HOME=$HOME/.cargo
export RUST_SRC_PATH=$HOME/.rustup/toolchains/nightly-2018-02-12-x86_64-apple-darwin/lib/rustlib/src/rust/src
