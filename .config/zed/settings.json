{
  "edit_predictions": {
    "disabled_globs": ["**/dnb.ghe.com/**", "**/gitlab.tech.dnb.no/**"],
    "mode": "eager"
  },
  "active_pane_modifiers": {
    "magnification": 1.4,
    "border_size": 1,
    "inactive_opacity": 0.81247
  },
  "assistant": {
    "default_profile": "manual",
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-7-sonnet-latest"
    },
    "default_height": 640.0,
    "version": "2",
    "default_width": 640
  },
  "buffer_font_family": "VictorMono Nerd Font",
  "buffer_font_size": 18,
  "chat_panel": {
    "default_width": 380
  },
  "features": {
    "copilot": false
  },
  "git": {
    "git_gutter": "tracked_files"
  },
  "git_hosting_providers": [
    {
      "provider": "github",
      "name": "DNB Github",
      "base_url": "https://dnb.ghe.com"
    },
    {
      "provider": "gitlab",
      "name": "DNB Gitlab",
      "base_url": "https://gitlab.tech.dnb.no"
    }
  ],
  "icon_theme": {
    "mode": "system",
    "light": "Catppuccin Latte",
    "dark": "Catppuccin Frappé"
  },
  "inlay_hints": {
    "enabled": false,
    "toggle_on_modifiers_press": { "control": true }
  },
  "languages": {
    "Python": {
      "language_servers": ["ruff"],
      "format_on_save": "on",
      "formatter": [
        {
          "code_actions": {
            "source.organizeImports.ruff": true,
            "source.fixAll.ruff": true
          }
        },
        {
          "language_server": {
            "name": "ruff"
          }
        }
      ]
    },
    "Shell Script": {
      "format_on_save": "on",
      "formatter": {
        "external": {
          "command": "shfmt",
          "arguments": []
        }
      }
    }
  },
  "load_direnv": "shell_hook",
  "lsp": {
    "ruff": {
      "initialization_options": {
        "settings": {
          "lint": {
            "extendSelect": ["I"]
          }
        }
      }
    },
    "terraform-ls": {
      "initialization_options": {
        "experimentalFeatures": {
          "prefillRequiredFields": true
        }
      }
    }
  },
  "pane_split_direction_horizontal": "down",
  "pane_split_direction_vertical": "right",
  "project_panel": {
    "default_width": 246,
    "indent_size": 16
  },
  "search": {
    "whole_word": false,
    "case_sensitive": false,
    "include_ignored": false,
    "regex": true
  },
  "slash_commands": {
    "docs": { "enabled": true },
    "project": { "enabled": true }
  },
  "soft_wrap": "editor_width",
  "tab_bar": {
    "show_nav_history_buttons": false,
    "show_tab_bar_buttons": false
  },
  "tabs": {
    "git_status": true,
    "file_icons": true,
    "show_close_button": "hidden"
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "terminal": {
    "alternate_scroll": "on",
    "default_height": 666,
    "font_size": 17,
    "shell": {
      "program": "fish"
    },
    "max_scroll_history_lines": 100000
  },
  "theme": {
    "mode": "system",
    "dark": "Catppuccin Frappé (teal)",
    "light": "Catppuccin Latte (teal)"
  },
  "ui_font_size": 18,
  "vim_mode": true
}
