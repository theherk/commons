{
  "assistant": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-latest"
    },
    "default_height": 640.0,
    "version": "2",
    "default_width": 640
  },
  "chat_panel": {
    "default_width": 380
  },
  "features": {
    "copilot": false
  },
  "git": {
    "git_gutter": "tracked_files",
    "inline_blame": {
      "enabled": false
    }
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "theme": {
    "mode": "system",
    "dark": "Catppuccin Mocha",
    "light": "Catppuccin Latte"
  },
  "vim_mode": true,
  "ui_font_size": 16,
  "buffer_font_family": "VictorMono NF",
  "buffer_font_size": 18,
  "project_panel": {
    "default_width": 246,
    "indent_size": 16
  },
  "search": {
    "whole_word": false,
    "case_sensitive": false,
    "include_ignored": false,
    "regex": true
  },
  "terminal": {
    "default_height": 420,
    "font_size": 16,
    "shell": {
      "program": "fish"
    },
    "toolbar": {
      "title": false
    },
    "max_scroll_history_lines": 100000
  }
}
